#app .venues.participants
	list-style-type none
	padding 0
	margin 0
	> li
		margin 0
		> h3
			text-align center
			margin 0
			padding 10px
			font-size 14px
			font-weight bold
		> ol
			list-style-type none
			padding 0
			margin 0
			> li
				> a
					display flex
					width 100%
					text-decoration none
					justify-content flex-start
					align-items center
					padding 5px 10px
					> img
						flex none
						width 40px
						height 40px
						background-color white
					> .name
						flex auto
						color black
						font-size 14px
						font-weight bold
						padding 0 10px
					> .time
						text-align right
						flex none
						width 80px
						color black
						font-size 14px
